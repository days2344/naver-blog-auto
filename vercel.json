{
  "version": 2,
  "builds": [
    {
      "src": "post_to_naver.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "headers": { "Content-Type": "image/x-icon" },
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "post_to_naver.py"
    }
  ]
}
